{
    "opts": {
            "destination": "docs_new",
            "encoding": "utf8",
            "package": "package.json",
            "recurse": true,
            "template": "utils/docs/template",
            "themeOpts": {
                "ignoreInheritedSymbols": true
            }
    },
    "plugins": [ "plugins/markdown" ],
    "source": {
        "include": [
            "examples/jsm/tsl",
            "src"
        ],
        "exclude": [
            "src/extras/PMREMGenerator.js"
        ]
    }
}
